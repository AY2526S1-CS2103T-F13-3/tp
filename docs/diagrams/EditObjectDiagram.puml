@startuml
title EditAthleteCommand Object Diagram

skinparam objectFontSize 15
skinparam defaultFontSize 15
skinparam ArrowColor #444444
skinparam ArrowThickness 1.1
skinparam ArrowFontStyle plain
skinparam shadowing false

' ==== Objects (Runtime Instances) ====
object userInput as "<u>userInput : String</u>" {
  value = "edit-a n/LeBron James s/Basketball e/lebron@newagency.com p/98765432"
}

object parser as "<u>parser : EditAthleteCommandParser</u>"

object command as "<u>command : EditAthleteCommand</u>" {
  name = "LeBron James"
  sport = "Basketball"
}

object descriptor as "<u>descriptor : EditAthleteDescriptor</u>" {
  phone = "98765432"
  email = "lebron@newagency.com"
  age = "(null)"
}

object model as "<u>model : Model</u>"

object targetAthlete as "<u>targetAthlete : Athlete</u>" {
  name = "LeBron James"
  sport = "Basketball"
  phone = "99887766"
  email = "james@example.com"
  age = "40"
}

object editedAthlete as "<u>editedAthlete : Athlete</u>" {
  name = "LeBron James"
  sport = "Basketball"
  phone = "98765432"
  email = "lebron@newagency.com"
  age = "40"
}

object result as "<u>result : CommandResult</u>" {
  feedbackToUser = "Edited athlete: LeBron James - Basketball"
}

' ==== Relationships (Directional) ====
userInput --> parser
parser --> "creates" command
command --> "contains" descriptor
command --> "executes" model
model --> "finds by \n name and sport " targetAthlete
descriptor --> "applies to" editedAthlete
model --> "replaces" editedAthlete
command --> "return        " result

@enduml
